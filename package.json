{
  "name": "kawaii",
  "version": "1.0.0",
  "description": "kawaii logs",
  "scripts": {
    "db:create": "sequelize-cli db:create",
    "db:drop": "sequelize-cli db:drop",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:migrate:create": "sequelize-cli migration:create",
    "db:seed": "sequelize-cli db:seed:all",
    "db:seed:create": "sequelize-cli seed:generate",
    "init": "npx sequelize db:drop && npx sequelize db:create && npx sequelize-cli db:migrate",
    "start": "nodemon src/index.js"
  },
  "keywords": [
    "nodejs",
    "express",
    "rest",
    "api",
    "sequelize",
    "mysql"
  ],
  "license": "ISC",
  "dependencies": {
    "@babel/node": "^7.13.12",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "ethers": "^5.4.6",
    "express": "^4.17.1",
    "express-validator": "^6.12.1",
    "fs": "^0.0.1-security",
    "helmet": "^4.6.0",
    "memory-cache": "^0.2.0",
    "morgan": "^1.10.0",
    "mysql2": "^2.0.2",
    "nodemon": "^2.0.12",
    "path": "^0.12.7",
    "sequelize": "^5.21.2",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "babel-node": "^0.0.1-security"
  }
}
